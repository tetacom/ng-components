{
  "name": "teta-components",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "semantic-release": "semantic-release",
    "components:sr": "cd dist/components semantic-release -e semantic-release-monorepo"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.3",
    "@angular/cdk": "^16.1.4",
    "@angular/common": "^16.1.3",
    "@angular/compiler": "^16.1.3",
    "@angular/core": "^16.1.3",
    "@angular/forms": "^16.1.3",
    "@angular/platform-browser": "^16.1.3",
    "@angular/platform-browser-dynamic": "^16.1.3",
    "@angular/router": "^16.1.3",
    "@maskito/angular": "^1.2.0",
    "@maskito/core": "^1.2.0",
    "@maskito/kit": "^1.2.0",
    "@ngneat/transloco": "^3.1.0",
    "@storybook/addon-knobs": "^7.0.2",
    "@tetacom/ng-components": "^1.1.9",
    "d3": "^7.1.1",
    "dayjs": "^1.11.7",
    "html-to-image": "^1.9.0",
    "luxon": "^3.3.0",
    "minimist": "^1.2.7",
    "object-hash": "^2.2.0",
    "rxjs": "^7.5.5",
    "style-dictionary": "^3.7.1",
    "three": "^0.139.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.4",
    "@angular/cli": "^16.1.4",
    "@angular/compiler-cli": "^16.1.3",
    "@angular/elements": "^16.1.3",
    "@compodoc/compodoc": "1.1.21",
    "@storybook/addon-essentials": "7.0.26",
    "@storybook/addon-interactions": "7.0.26",
    "@storybook/addon-links": "7.0.26",
    "@storybook/addon-styling": "^1.0.0-next.2",
    "@storybook/angular": "7.0.26",
    "@storybook/blocks": "7.0.26",
    "@storybook/testing-library": "0.0.14-next.2",
    "@tetacom/themes": "^0.0.14",
    "@types/d3": "^7.1.0",
    "@types/jasmine": "~3.10.0",
    "@types/luxon": "^3.1.0",
    "@types/node": "^20.4.0",
    "@types/resize-observer-browser": "^0.1.6",
    "@types/three": "^0.139.0",
    "@webcomponents/custom-elements": "^1.5.0",
    "ajv-formats": "^2.1.1",
    "faker": "^5.5.3",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^16.1.0",
    "prettier": "^2.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "semantic-release": "^18.0.0",
    "semantic-release-monorepo": "^7.0.5",
    "storybook": "7.0.26",
    "style-loader": "^3.3.3",
    "typescript": "~4.9.5"
  },
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "preset": "conventionalcommits",
        "releaseRules": [
          {
            "type": "*!",
            "release": "major"
          }
        ]
      }
    ],
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    [
      "@semantic-release/github",
      {
        "assets": [
          "dist/**"
        ]
      }
    ],
    [
      "@semantic-release/npm",
      {
        "pkgRoot": "dist"
      }
    ],
    "@semantic-release/git"
  ],
  "preset": "angular",
  "release": {
    "branches": [
      "main",
      "main-fix-old-version",
      "update-date-cell"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tetacom/ng-components.git"
  }
}
