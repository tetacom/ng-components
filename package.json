{
  "name": "teta-components",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d ./compodoc",
    "sb": "yarn docs:json && start-storybook -p 6006",
    "build-storybook": "yarn docs:json && build-storybook -o dist/storybook",
    "semantic-release": "semantic-release",
    "components:sr": "cd dist/components semantic-release -e semantic-release-monorepo"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.0",
    "@angular/cdk": "^13.0.0",
    "@angular/common": "~13.0.0",
    "@angular/compiler": "~13.0.1",
    "@angular/core": "~13.0.0",
    "@angular/forms": "~13.0.0",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/router": "~13.0.0",
    "@ngneat/transloco": "^3.1.0",
    "d3": "^7.1.1",
    "ngx-ui-scroll": "^2.3.0",
    "object-hash": "^2.2.0",
    "rxjs": "~7.4.0",
    "three": "^0.134.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.2",
    "@angular/cli": "~13.0.2",
    "@angular/compiler-cli": "~13.0.1",
    "@angular/elements": "^13.0.0",
    "@babel/core": "^7.16.0",
    "@compodoc/compodoc": "^1.1.15",
    "@storybook/addon-actions": "^6.5.0-alpha.5",
    "@storybook/addon-docs": "^6.5.0-alpha.5",
    "@storybook/addon-essentials": "^6.5.0-alpha.5",
    "@storybook/addon-knobs": "^6.3.1",
    "@storybook/addon-links": "^6.5.0-alpha.5",
    "@storybook/angular": "^6.5.0-alpha.5",
    "@storybook/builder-webpack5": "^6.5.0-alpha.5",
    "@storybook/manager-webpack5": "^6.5.0-alpha.5",
    "@types/d3": "^7.1.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@types/resize-observer-browser": "^0.1.6",
    "@types/three": "^0.134.0",
    "@webcomponents/custom-elements": "^1.5.0",
    "ajv-formats": "^2.1.1",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "d3-svg-annotation": "git+http://github.com/andreyelpaev/d3-annotation.git",
    "d3-tricontour": "^1.0.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "faker": "^5.5.3",
    "html-webpack-plugin": "^5.5.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "mini-css-extract-plugin": "^2.4.1",
    "ng-packagr": "^13.0.3",
    "node-sass": "^6.0.1",
    "prettier": "^2.4.1",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "semantic-release": "^18.0.0",
    "semantic-release-monorepo": "^7.0.5",
    "storybook-addon-themes": "^6.1.0",
    "style-loader": "^3.3.1",
    "typescript": "~4.4.3",
    "webpack": "^5.64.0",
    "webpack-cli": "^4.9.1"
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    [
      "@semantic-release/github",
      {
        "assets": [
          "dist/**"
        ]
      }
    ],
    [
      "@semantic-release/npm",
      {
        "pkgRoot": "dist"
      }
    ],
    "@semantic-release/git"
  ],
  "preset": "angular",
  "release": {
    "branches": [
      "main"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tetacom/ng-components.git"
  }
}
